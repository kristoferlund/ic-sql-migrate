{
  "canisters": {
    "turso-example": {
      "candid": "examples/turso/turso-example.did",
      "package": "turso",
      "declarations": {
        "output": "examples/turso/declarations"
      },
      "build": [
        "cargo build -p turso-example --target wasm32-wasip1 --release",
        "wasi2ic target/wasm32-wasip1/release/turso_example.wasm target/wasm32-wasip1/release/turso-example-wasi2ic.wasm"
      ],
      "wasm": "target/wasm32-wasip1/release/turso-example-wasi2ic.wasm",
      "type": "custom",
      "metadata": [
        {
          "name": "candid:service"
        }
      ]
    },
    "sqlite-example": {
      "candid": "examples/sqlite/sqlite-example.did",
      "package": "sqlite",
      "declarations": {
        "output": "examples/sqlite/declarations"
      },
      "build": [
        "cargo build -p sqlite-example --target wasm32-wasip1 --release",
        "wasi2ic target/wasm32-wasip1/release/sqlite_example.wasm target/wasm32-wasip1/release/sqlite-example-wasi2ic.wasm"
      ],
      "wasm": "target/wasm32-wasip1/release/sqlite-example-wasi2ic.wasm",
      "type": "custom",
      "metadata": [
        {
          "name": "candid:service"
        }
      ]
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
